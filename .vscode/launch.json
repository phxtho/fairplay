  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'fairplay' server",
      "cargo": {
        "args": ["build", "--bin=fairplay", "--package=fairplay"],
        "filter": {
          "name": "fairplay",
          "kind": "bin"
        }
      },
      "args": ["server"],
      "cwd": "${workspaceFolder}"
    },
  ]
}
